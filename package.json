{
  "name": "guabaovlang",
  "displayName": "GuaBaoVLang",
  "description": "GuaBao Verification Language and Tools",
  "publisher": "scmlab",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "guabao",
        "extensions": [
          ".gcl",
          ".guabao"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "guabao",
        "scopeName": "source.gcl",
        "path": "./syntaxes/guabao.tmGrammar.json"
      }
    ],
    "commands": [
      {
        "command": "guabaovlang.greet",
        "title": "GuaBao Greet"
      },
      {
        "command": "guabaovlang.start",
        "title": "GuaBao Start"
      },
      {
        "command": "guabaovlang.stop",
        "title": "GuaBao Stop"
      },
      {
        "command": "guabaovlang.restart",
        "title": "GuaBao Restart"
      },
      {
        "command": "guabaovlang.refine",
        "title": "GuaBao Refine"
      },
      {
        "command": "guabaovlang.debug",
        "title": "GuaBao Debug"
      },
      {
        "command": "guabaovlang.vcgen",
        "title": "GuaBao Generate VC"
      },
      {
        "command": "guabaovlang.solve",
        "title": "GuaBao Solve"
      }
    ],
    "keybindings": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
